{
  "name": "mais_envios",
  "version": "1.0.0",
  "description": "An API built with NodeJS, Express and TypeScript, for tag control",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon ./src/app.ts",
    "start": "nodemon ./dist/app.js",
    "prod": "npm run build && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "watch": [
      "server"
    ],
    "ext": "ts",
    "ignore": [
      "*.test.ts"
    ],
    "delay": "3",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "keywords": [],
  "author": "Jos√© Roberto Miguel Filho <joseroberto.toko@gmail.com.com> (https://www.linkedin.com/in/joserobertotoko/)",
  "license": "ISC",
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^1.6.7",
    "config": "^3.3.11",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "http-status-codes": "^2.3.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/config": "^3.3.3",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.11.16",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "ts-node-dev": "^2.0.0"
  }
}
